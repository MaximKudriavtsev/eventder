{
  "name": "eventder",
  "version": "0.0.1",
  "description": "Eventder App",
  "repository": "https://github.com/MaximKudriavtsev/eventder",
  "author": "no@no.net",
  "license": "Licence",
  "private": true,
  "scripts": {
    "start": "node scraper-vk/index.js && parcel index.html --port 3000",
    "build": "BASE_NAME=/site/ parcel build index.html --public-url /site/",
    "lint": "eslint .",
    "prettier": "prettier --write src/**/*.js",
    "stylelint": "stylelint '**/*.scss'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add",
      "yarn lint"
    ],
    "*.scss": [
      "yarn stylelint"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "express": "^4.16.4",
    "formidable": "^1.2.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "parcel-bundler": "^1.12.3",
    "postcss-camel-case": "^1.0.1",
    "prettier": "^1.17.0",
    "sass": "^1.18.0",
    "stylelint": "^10.0.0",
    "stylelint-config-standard": "^18.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "dependencies": {
    "@urbica/react-map-gl": "^1.5.0",
    "@urbica/react-map-gl-cluster": "^0.1.0",
    "autoprefixer": "^9.5.1",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.23.0",
    "js-cookie": "^2.2.0",
    "leaflet": "1.4.0",
    "leaflet.markercluster": "^1.4.1",
    "mapbox-gl": "^0.53.1",
    "node-jose": "^1.1.3",
    "postcss-modules": "^1.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-leaflet": "^2.2.1",
    "react-redux": "^7.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^8.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  }
